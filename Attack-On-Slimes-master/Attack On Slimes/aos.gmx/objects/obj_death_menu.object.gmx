          <argument>
            <kind>1</kind>
            <string>/// control menu
if (alarm[0] &lt;= 0){    
    if (obj_input.down){
        if (menu_index &lt; array_length_1d(option)-1){
            menu_index++;
        }
        else {
            menu_index = 0;
        }
        alarm [0] = room_speed/6;
    }
    if (obj_input.up){
        if (menu_index &gt; 0){
            menu_index--;
        }
        else {
            menu_index = array_length_1d(option)-1;
        }
        alarm [0] = room_speed/3;
    }
    
    if (obj_input.select){
        switch (menu_index) {
            case 0:
                room_goto(lab1);
                break;
            
            case 1:
                room_goto(rm_title_screen);
                break;
                
            case 2:
                game_end();
                break;
                
            default:
                break;
        }
    }
    
}
